{
    "name": "@recursyve/nestjs-rosetta",
    "version": "1.0.0",
    "scripts": {
        "build": "gulp build --dist node_modules/@recursyve",
        "prebuild:prod": "npm run clean",
        "build:prod": "gulp build",
        "test": "jest",
        "clean": "gulp clean:bundle",
        "prerelease": "gulp copy-misc && gulp build --dist node_modules/@recursyve",
        "publish": "npm run prerelease && npm run build:prod && ./node_modules/.bin/lerna publish --force-publish --exact -m \"Publish %s release\"",
        "publish:beta": "npm run prerelease && npm run build:prod && ./node_modules/.bin/lerna publish --dist-tag=beta -m \"Publish %s release\""
    },
    "peerDependencies": {
        "@nestjs/common": "^10.0.0",
        "@nestjs/core": "^10.0.0",
        "rxjs": ">=7.5.5",
        "accept-language-parser": "^1.5.0",
        "reflect-metadata": "^0.1.13"
    },
    "devDependencies": {
        "@nestjs/common": "^10.2.7",
        "@nestjs/core": "^10.2.7",
        "@nestjs/mongoose": "^10.0.1",
        "@nestjs/platform-express": "^10.2.7",
        "@nestjs/sequelize": "^10.0.0",
        "@types/accept-language-parser": "^1.5.4",
        "@types/body-parser": "^1.19.3",
        "@types/connect": "^3.4.36",
        "@types/debug": "^4.1.9",
        "@types/express": "^4.17.18",
        "@types/jest": "^29.5.5",
        "@types/ms": "^0.7.32",
        "@types/node": "^18.18.3",
        "@types/validator": "^13.11.2",
        "accept-language-parser": "^1.5.0",
        "delete-empty": "^3.0.0",
        "gulp": "^4.0.2",
        "gulp-clean": "^0.4.0",
        "gulp-sourcemaps": "^3.0.0",
        "gulp-typescript": "^6.0.0-alpha.1",
        "jest": "^29.7.0",
        "lerna": "^6.4.0",
        "mongoose": "^7.5.4",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.0",
        "sequelize": "^6.33.0",
        "sequelize-typescript": "^2.1.5",
        "sqlite3": "^5.1.6",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "tslint": "^6.1.3",
        "typescript": "^5.2.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "packages",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "./coverage",
        "testEnvironment": "node",
        "collectCoverage": true
    },
    "dependencies": {
        "@nestjs/mongoose": "^9.2.1"
    }
}
